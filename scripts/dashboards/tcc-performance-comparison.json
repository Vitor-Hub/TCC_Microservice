{
  "dashboard": {
    "title": "TCC - Comparação de Performance: Microsserviços vs Monolito",
    "panels": [
      {
        "id": 1,
        "title": "Throughput (Requisições/segundo)",
        "targets": [
          {
            "expr": "rate(http_server_requests_seconds_count{job=\"apps\"}[1m])",
            "legendFormat": "{{app}}"
          }
        ]
      },
      {
        "id": 2,
        "title": "Latência P95 por Aplicação",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_server_requests_seconds_bucket{job=\"apps\"}[1m]))",
            "legendFormat": "{{app}}"
          }
        ]
      },
      {
        "id": 3,
        "title": "Uso de CPU (%)",
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total{job=\"apps\"}[1m]) * 100",
            "legendFormat": "{{app}}"
          }
        ]
      },
      {
        "id": 4,
        "title": "Uso de Memória (MB)",
        "targets": [
          {
            "expr": "jvm_memory_used_bytes{area=\"heap\",job=\"apps\"} / 1024 / 1024",
            "legendFormat": "{{app}}"
          }
        ]
      }
    ]
  }
}